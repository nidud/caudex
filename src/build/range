# RANGE--
#
# range(u[], min, max)
#
s = ToString("# range " + ToString(UCodePoint($2)) + "-" + ToString(UCodePoint($3)))
Print(s)
WriteStringToFile(s + " \n", "glist", 1)
i = 0
c = SizeOf($1)
while ( i < c )
    if ( $1[i] >= $2 && $1[i] <= $3 )
        addgl($1[i])
        $1[i] = 0
    endif
    ++i
endloop

