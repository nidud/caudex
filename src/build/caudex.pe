# CAUDEX.PE--
#
create()
if ( $argc == 2 )
    version = $1
else
    version = "2.0"
endif
target = ToString("../caudex-" + version + ".sfd")
Print("Building " + target)
Open("caudex.sfd", 1)
Reencode("unicode")
Validate()
ulink(0xFF0000)
ulink(0x00FF00)
clean()
fixup(0u203B)
Reencode("compacted")
Save(target)
Close()
Quit()
